<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Plate.
    -->
    <changeSet id="20201225142030-2" author="freedata">

        <addForeignKeyConstraint baseColumnNames="plate_id"
                                 baseTableName="plate_note"
                                 constraintName="fk_plate_note_plate_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plate"/>

        <addForeignKeyConstraint baseColumnNames="note_id"
                                 baseTableName="plate_note"
                                 constraintName="fk_plate_note_note_id"
                                 referencedColumnNames="id"
                                 referencedTableName="note"/>

        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="plate"
                                 constraintName="fk_plate_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

    </changeSet>
</databaseChangeLog>
